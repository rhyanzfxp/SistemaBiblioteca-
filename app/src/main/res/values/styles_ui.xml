<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Text.TitleLarge" parent="TextAppearance.Material3.TitleLarge">
        <item name="android:textColor">@color/lib_text_primary</item>
    </style>
    <style name="Text.TitleMedium" parent="TextAppearance.Material3.TitleMedium">
        <item name="android:textColor">@color/lib_text_primary</item>
    </style>
    <style name="Text.Body" parent="TextAppearance.Material3.BodyMedium">
        <item name="android:textColor">@color/lib_text_secondary</item>
    </style>
    <style name="Button.Primary" parent="Widget.Material3.Button">
        <item name="backgroundTint">@color/lib_secondary</item>
        <item name="android:textColor">@color/lib_white</item>
        <item name="cornerRadius">16dp</item>
        <item name="elevation">2dp</item>
    </style>
    <style name="Button.Outlined" parent="Widget.Material3.Button.OutlinedButton">
        <item name="strokeColor">@color/lib_secondary</item>
        <item name="strokeWidth">1dp</item>
        <item name="android:textColor">@color/lib_secondary</item>
        <item name="cornerRadius">16dp</item>
    </style>
    <style name="Card.Container" parent="Widget.MaterialComponents.CardView">
        <item name="cardUseCompatPadding">true</item>
        <item name="cardCornerRadius">20dp</item>
        <item name="cardBackgroundColor">@color/lib_white</item>
        <item name="cardElevation">4dp</item>
    </style>
    <style name="Chip.Filter" parent="Widget.MaterialComponents.Chip.Filter">
        <item name="android:textColor">@color/lib_text_secondary</item>
    </style>
</resources>